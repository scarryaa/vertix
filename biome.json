{
	"$schema": "https://biomejs.dev/schemas/1.6.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true
		}
	},
	"javascript": {
		"formatter": {
			"enabled": true
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	},
	"css": {
		"formatter": {
			"enabled": true
		}
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"defaultBranch": "rewrite",
		"useIgnoreFile": false
	}
}
