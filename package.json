{
	"name": "vertix",
	"version": "0.0.1",
	"description": "Vertix server implementation for Codestash",
	"main": "index.js",
	"scripts": {
		"start": "ts-node src/index.ts",
		"test": "pnpm jest",
		"biome:format:fix": "pnpm biome format --write ./",
		"biome:lint:fix": "pnpm biome lint --apply ./",
		"biome:fix-all": "pnpm biome:lint:fix; pnpm biome:format:fix",
		"db-seed": "tsc prisma/seed.ts && cat prisma/seed.js | node --input-type=\"commonjs\" && rm prisma/seed.js",
		"db-reset": "prisma db push --force-reset",
		"db-migration": "prisma migrate dev --name $npm_config_name",
		"db-schema": "cd ./prisma/schema; cat db.part.prisma enums.part.prisma models.part.prisma > ../schema.prisma"
	},
	"keywords": [
		"vertix",
		"codestash"
	],
	"author": "scarryaa",
	"license": "MIT",
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
		"@fastify/cookie": "^9.3.1",
		"@fastify/env": "^4.3.0",
		"@fastify/formbody": "^7.4.0",
		"@fastify/rate-limit": "^9.1.0",
		"@prisma/client": "^5.6.0",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.4.5",
		"fastify": "^4.26.2",
		"fastify-zod": "^1.4.0",
		"jsonwebtoken": "^9.0.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.4",
		"@types/bcrypt": "^5.0.2",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^16.18.94",
		"@types/node-fetch": "^2.6.11",
		"jest": "^29.7.0",
		"jest-mock-extended": "3.0.5",
		"node-fetch": "^2.7.0",
		"prisma": "^5.6.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.2.1",
		"typescript": "^5.4.3"
	}
}
